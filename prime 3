int main()
{
    int i,num = 21;
    int isPrime= 1;
    
    
    
    if(num < 2){
        isPrime = 0;
    }
    else{   
        
        for(i=2; i < num; i++)
        {
            if(num % i == 0)
            {
                isPrime = 0;
                break;
            }
        }
    }

    if(isPrime)
        printf("%d is Prime", num);
    else
        printf("%d is not Prime", num);
 
    return 0;
}
